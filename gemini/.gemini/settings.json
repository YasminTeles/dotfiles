{
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "previewFeatures": true
  },
  "mcpServers": {
    "Github": {
      "httpUrl": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${GITHUB_TOKEN}"
      },
      "timeout": 5000,
      "trust": true
    },
    "Notion": {
      "httpUrl": "https://mcp.notion.com/mcp",
      "trust": true
    },
    "Jira": {
      "url": "https://mcp.atlassian.com/v1/sse",
      "trust": true
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "Default"
  }
}
